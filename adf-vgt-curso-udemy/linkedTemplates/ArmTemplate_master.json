{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-vgt-curso-udemy"
		},
		"ls_adls_udemy_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'ls_adls_udemy'"
		},
		"ls_mysql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_mysql'"
		},
		"ls_adls_udemy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stodfvagetti.dfs.core.windows.net/"
		},
		"ls_adls_udemy_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f9a807dd-535c-4672-b63b-4e3b809fdd0b"
		},
		"ls_adls_udemy_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "bd902dd9-cc1f-4896-a1c4-72c030f9c7a4"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-vgt-curso-udemy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_udemy_servicePrincipalCredential": {
						"value": "[parameters('ls_adls_udemy_servicePrincipalCredential')]"
					},
					"ls_mysql_connectionString": {
						"value": "[parameters('ls_mysql_connectionString')]"
					},
					"ls_adls_udemy_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_url')]"
					},
					"ls_adls_udemy_properties_typeProperties_tenant": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_tenant')]"
					},
					"ls_adls_udemy_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}